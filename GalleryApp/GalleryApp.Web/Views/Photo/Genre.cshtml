@model IEnumerable<GalleryApp.Domain.Models.Photo>

@{
    ViewData["Title"] = "Details";
}

<div class="am-container" id="am-container">
    @foreach (var img in Model)
    {
        <a data-fancybox="gallery" href="/Images/@img.Name">
            <img class="imgall" src="/Images/thumbnails/@img.Name" alt="@img.Title">
        </a>
    }
</div>
<script src="https://snipp.ru/cdn/fancybox/3.5.7/jquery.fancybox.min.js"></script>